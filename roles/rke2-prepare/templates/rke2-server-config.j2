write-kubeconfig-mode: "0644"
{% if inventory_hostname != groups['servers'][0] %}
server: https://{{ vip }}:9345
{% endif %}
tls-san:
  - {{ vip }}
{% for host in groups['servers'] %}
  - {{ hostvars[host]['ansible_host'] }}
{% endfor %}
node-label:
  - server=true
#disable:
#  - rke2-ingress-nginx
cni: {{ cni }}
